<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item xmlns:android="http://schemas.android.com/apk/res/android">
        <shape
            android:shape="oval">
            <solid android:color="@android:color/white"/>
            <stroke android:width="1dp"
                android:color="@color/status_circle_container" />
        </shape>
    </item>

    <item xmlns:android="http://schemas.android.com/apk/res/android"
        android:left="@dimen/work_status_circle_stroke_padding"
        android:right="@dimen/work_status_circle_stroke_padding"
        android:top="@dimen/work_status_circle_stroke_padding"
        android:bottom="@dimen/work_status_circle_stroke_padding">
        <shape
            android:shape="oval">
            <solid android:color="@color/status_circle_passed"
                />
        </shape>
    </item>

</layer-list>

